Schema de.uni_koblenz.jgralabtest.schemas.constrained.ConstrainedSchema;
GraphClass ConstrainedGraph;
VertexClass ConstrainedNode { name : String, uid : Integer } ["from n : V{ConstrainedNode} with n.name = null reportSet n, \"Attribute name is not set.\" end", "forall n : V{ConstrainedNode} @ n.uid > 0"];
VertexClass OtherConstrainedNode : ConstrainedNode {niceness : Integer} ["from n : V{OtherConstrainedNode} with n.niceness < 0 or n.niceness > 20 reportSet n, \"Attribute niceness is \" + n.niceness + \" but only values between 0 and 20 are allowed.\" end"];
EdgeClass ConstrainedLink from ConstrainedNode (0,*) to ConstrainedNode (0,*) {uid : Integer} ["from l : E{ConstrainedLink} with l.uid <= 0 reportSet l, \"Attribute uid is \" + l.uid + \" but should be > 0.\" end"];
